#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/petunia/code/Algorithm/frontend/projects/vue3-components-lib/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.44.0_@types+node@24.10.1_postcss@8.5.6_sass@1._bc1337aabbf725c1adb857051a48f75e/node_modules/vitepress/bin/node_modules:/Users/petunia/code/Algorithm/frontend/projects/vue3-components-lib/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.44.0_@types+node@24.10.1_postcss@8.5.6_sass@1._bc1337aabbf725c1adb857051a48f75e/node_modules/vitepress/node_modules:/Users/petunia/code/Algorithm/frontend/projects/vue3-components-lib/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.44.0_@types+node@24.10.1_postcss@8.5.6_sass@1._bc1337aabbf725c1adb857051a48f75e/node_modules:/Users/petunia/code/Algorithm/frontend/projects/vue3-components-lib/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/petunia/code/Algorithm/frontend/projects/vue3-components-lib/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.44.0_@types+node@24.10.1_postcss@8.5.6_sass@1._bc1337aabbf725c1adb857051a48f75e/node_modules/vitepress/bin/node_modules:/Users/petunia/code/Algorithm/frontend/projects/vue3-components-lib/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.44.0_@types+node@24.10.1_postcss@8.5.6_sass@1._bc1337aabbf725c1adb857051a48f75e/node_modules/vitepress/node_modules:/Users/petunia/code/Algorithm/frontend/projects/vue3-components-lib/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.44.0_@types+node@24.10.1_postcss@8.5.6_sass@1._bc1337aabbf725c1adb857051a48f75e/node_modules:/Users/petunia/code/Algorithm/frontend/projects/vue3-components-lib/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress.js" "$@"
fi
